\end{tabular}

T
SELECT DISTINCT age, lastname FROM people ORDER BY lastname, age DESC LIMIT 1

T
SELECT age, trim(lastname) FROM people GROUP BY age, lastname ORDER BY max(age) DESC, lastname DESC LIMIT 1

F
SELECT age, trim(lastname) FROM people ORDER BY oid LIMIT 1

F
SELECT age, lastname FROM people WHERE lastname = 'Иванов' ORDER BY upper(lastname) DESC, age DESC LIMIT 1

F
SELECT age, lower(lastname) FROM people ORDER BY oid LIMIT 1

