\end{tabular}

T
SELECT firstname, secondname, lower(lastname), id, age FROM people ORDER BY secondname LIMIT 1

F
SELECT firstname, substring(secondname, 4, 9), lastname, id, age FROM people ORDER BY oid LIMIT 1

F
SELECT firstname, upper(secondname), lower(lastname), id, age FROM people WHERE lastname = 'Иванов' AND secondname like 'Иванович' ORDER BY trim(secondname), age DESC, lower(firstname) ASC, id DESC LIMIT 1

F
SELECT DISTINCT upper(firstname), lower(secondname), lastname, id, age FROM people WHERE lastname = 'Иванов' AND id in (1, 2) AND age in (30, 40) AND firstname in ('Иван', 'Петр') AND secondname in ('Иванович', 'Петрович') ORDER BY id ASC LIMIT 1

F
SELECT DISTINCT firstname, trim(secondname), substring(lastname, 5, 9), id, age FROM people WHERE firstname like 'Иван' ORDER BY age ASC LIMIT 1

