T
SELECT secondname FROM people WHERE secondname like 'Иванович' GROUP BY secondname ORDER BY lower(secondname) DESC LIMIT 1

T
SELECT secondname FROM people WHERE secondname = 'Иванович' ORDER BY secondname DESC LIMIT 1

T
SELECT secondname FROM people ORDER BY oid LIMIT 1

T
SELECT trim(secondname) FROM people WHERE secondname like 'Иванович' ORDER BY oid LIMIT 1

F
SELECT DISTINCT secondname FROM people WHERE secondname in ('Иванович', 'Петрович') ORDER BY secondname DESC LIMIT 1

