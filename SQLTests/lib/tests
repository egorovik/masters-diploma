\end{tabular}

T
SELECT id, firstname, trim(secondname) FROM people WHERE id in (1, 2) AND secondname = 'Иванович' AND firstname in ('Иван', 'Петр') ORDER BY oid LIMIT 1

T
SELECT id, firstname, secondname FROM people WHERE firstname = 'Иван' ORDER BY firstname LIMIT 1

T
SELECT id, firstname, secondname FROM people ORDER BY firstname LIMIT 1

F
SELECT id, lower(firstname), secondname FROM people WHERE secondname = 'Иванович' GROUP BY id, firstname, secondname ORDER BY secondname DESC, id ASC, lower(firstname) DESC LIMIT 1

F
SELECT id, substring(firstname, 2, 8), trim(secondname) FROM people ORDER BY id ASC, firstname ASC, secondname DESC LIMIT 1

