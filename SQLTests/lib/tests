F
SELECT count(age), secondname, lastname FROM people WHERE lastname = 'Иванов' GROUP BY secondname, lastname ORDER BY lastname DESC, min(age) ASC, secondname ASC LIMIT 1

F
SELECT max(age), secondname, lower(lastname) FROM people WHERE age in (30, 40) AND secondname like 'Иванович' AND lastname = 'Иванов' GROUP BY secondname, lastname ORDER BY secondname ASC, min(age) DESC, lastname DESC LIMIT 1

F
SELECT age, upper(secondname), count(lastname) FROM people GROUP BY age, secondname ORDER BY substring(secondname, 3, 3), cast(avg(age) as integer) ASC LIMIT 1

