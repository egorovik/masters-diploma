Иван & Иванов & 1 & 30 & Иванович\\
\hline
\end{tabular}

T
SELECT trim(firstname), lastname, id, age, secondname FROM people ORDER BY id, age DESC, lastname ASC, secondname ASC, upper(firstname) LIMIT 1

F
SELECT lower(firstname), substring(lastname, 1, 4), id, age, upper(secondname) FROM people ORDER BY oid LIMIT 1

F
SELECT firstname, trim(lastname), id, max(age), upper(secondname) FROM people GROUP BY firstname, lastname, id, secondname ORDER BY max(age), id ASC, firstname LIMIT 1

F
SELECT firstname, lastname, id, age, lower(secondname) FROM people WHERE lastname = 'Иванов' AND age in (30, 40) AND secondname in ('Иванович', 'Петрович') AND id = 1 ORDER BY trim(secondname) ASC, id DESC, age DESC, lastname DESC, firstname LIMIT 1

F
SELECT upper(firstname), lastname, id, age, substring(secondname, 4, 2) FROM people WHERE lastname in ('Иванов', 'Петров') ORDER BY oid LIMIT 1

