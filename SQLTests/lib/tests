\end{tabular}

T
SELECT firstname, age, id FROM people WHERE firstname = 'Иван' AND id in (1, 2) GROUP BY firstname, age, id ORDER BY sum(id) ASC, min(age), firstname DESC LIMIT 1

T
SELECT firstname, age, id FROM people WHERE firstname = 'Иван' ORDER BY oid LIMIT 1

T
SELECT firstname, age, id FROM people ORDER BY firstname ASC, age, id ASC LIMIT 1

F
