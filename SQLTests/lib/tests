\end{tabular}

T
SELECT firstname, id FROM people WHERE firstname in ('Иван', 'Петр') ORDER BY oid LIMIT 1

T
SELECT firstname, id FROM people GROUP BY firstname, id ORDER BY min(id) LIMIT 1

T
SELECT firstname, id FROM people WHERE firstname like 'Иван' ORDER BY id DESC, firstname DESC LIMIT 1

F
SELECT upper(firstname), id FROM people WHERE id = 1 OR firstname like 'Иван' GROUP BY firstname, id ORDER BY id DESC LIMIT 1

F
SELECT DISTINCT firstname, id FROM people WHERE id in (1, 2) ORDER BY firstname DESC LIMIT 1

