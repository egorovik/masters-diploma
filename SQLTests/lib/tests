F
SELECT upper(lastname), id, age, firstname FROM people ORDER BY firstname LIMIT 1

F
SELECT substring(lastname, 5, 5), id, age, firstname FROM people WHERE lastname like 'Иванов' ORDER BY oid LIMIT 1

F
SELECT substring(lastname, 2, 10), id, age, substring(firstname, 4, 5) FROM people WHERE id in (1, 2) AND lastname in ('Иванов', 'Петров') ORDER BY oid LIMIT 1

