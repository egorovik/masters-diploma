\end{tabular}

T
SELECT lastname, secondname, id, age FROM people ORDER BY oid LIMIT 1

T
SELECT lastname, trim(secondname), id, age FROM people WHERE id = 1 AND secondname like 'Иванович' AND age = 30 AND lastname in ('Иванов', 'Петров') GROUP BY lastname, secondname, id, age ORDER BY lastname DESC, lower(secondname) ASC, age, id DESC LIMIT 1

F
SELECT trim(lastname), substring(secondname, 3, 5), id, count(age) FROM people WHERE age = 30 AND lastname like 'Иванов' AND id = 1 AND secondname in ('Иванович', 'Петрович') GROUP BY lastname, secondname, id ORDER BY secondname LIMIT 1

F
SELECT lower(lastname), trim(secondname), id, age FROM people ORDER BY age ASC, id DESC LIMIT 1

F
SELECT substring(lastname, 4, 2), secondname, id, age FROM people ORDER BY oid LIMIT 1

