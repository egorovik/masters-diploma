\end{tabular}

T
SELECT min(age), lastname, count(upper(firstname)), count(upper(secondname)), count(id) FROM people GROUP BY lastname HAVING sum(age) > 0 ORDER BY cast(avg(age) as integer) ASC LIMIT 1

F
SELECT age, lastname, lower(firstname), secondname, id FROM people WHERE secondname = 'Иванович' OR firstname = 'Иван' OR age in (30, 40) OR lastname in ('Иванов', 'Петров') ORDER BY oid LIMIT 1

F
SELECT age, lastname, upper(firstname), secondname, id FROM people ORDER BY id, firstname DESC, secondname DESC LIMIT 1

F
SELECT DISTINCT age, lastname, lower(firstname), lower(secondname), id FROM people ORDER BY lastname ASC, lower(secondname) ASC LIMIT 1

F
SELECT age, trim(lastname), firstname, secondname, id FROM people WHERE secondname like 'Иванович' ORDER BY oid LIMIT 1

