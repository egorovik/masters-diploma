R

R2
index1

R

R1
SELECT

R

R2
GROUP BY field1, field2, field3 ORDER BY field1 DESC, field2 ASC, field3 DESC LIMIT 33

R

R1
DELETE

R

R2
WHERE  field1 > field2 AND field3 like '\%F\%'

