F
SELECT upper(lastname), id, firstname, age, trim(secondname) FROM people ORDER BY oid LIMIT 1

F
SELECT DISTINCT upper(lastname), id, firstname, age, secondname FROM people ORDER BY id DESC, age ASC LIMIT 1

F
SELECT substring(lastname, 1, 6), id, upper(firstname), age, lower(secondname) FROM people ORDER BY oid LIMIT 1

