                    ON(t2.childid =
                       t3.id)  LEFT OUTER JOIN people t1
                    ON(t3.parentid =
                       t1.id) ORDER BY trim(t2.lastname) ASC, t1.lastname DESC, t3.firstname DESC LIMIT 1}

