                    ON(t2.id =
                       t3.parentid)  RIGHT OUTER JOIN people t1
                    ON(t3.id =
                       t1.childid) WHERE t2.lastname like 'Ivanov' ORDER BY t1.childid, t1.lastname, t2.id ASC, upper(t2.lastname) LIMIT 1}

