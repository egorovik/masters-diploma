\end{tabular}

T
SELECT id, substring(lastname, 2, 4), substring(secondname, 4, 4), trim(firstname) FROM people WHERE firstname = 'Иван' OR secondname like 'Иванович' OR lastname = 'Иванов' ORDER BY upper(lastname) ASC, upper(firstname) ASC, id DESC, trim(secondname) LIMIT 1

F
SELECT id, substring(lastname, 5, 9), upper(secondname), lower(firstname) FROM people ORDER BY oid LIMIT 1

F
SELECT id, lastname, secondname, lower(firstname) FROM people WHERE id in (1, 2) AND secondname like 'Иванович' AND firstname = 'Иван' ORDER BY lastname DESC, id DESC, secondname ASC LIMIT 1

F
SELECT id, lastname, substring(secondname, 5, 9), firstname FROM people WHERE lastname like 'Иванов' OR secondname like 'Иванович' ORDER BY lastname ASC, id LIMIT 1

F
SELECT id, lastname, secondname, firstname FROM people ORDER BY id ASC, lower(firstname) LIMIT 1

