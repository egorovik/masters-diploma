SELECT id FROM people ORDER BY oid LIMIT 1

T
SELECT id FROM people WHERE id = 1 GROUP BY id ORDER BY min(id) LIMIT 1

T
SELECT id FROM people WHERE id = 1 ORDER BY oid LIMIT 1

