\hline
\end{tabular}

T
SELECT upper(secondname) FROM people ORDER BY oid LIMIT 1

T
SELECT upper(secondname) FROM people WHERE secondname = 'Иванович' ORDER BY trim(secondname) LIMIT 1

F
SELECT secondname FROM people ORDER BY secondname LIMIT 1

F
SELECT trim(secondname) FROM people WHERE secondname in ('Иванович', 'Петрович') ORDER BY secondname ASC LIMIT 1

F
SELECT secondname FROM people WHERE secondname = 'Иванович' ORDER BY secondname LIMIT 1

