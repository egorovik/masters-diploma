\end{tabular}

T
SELECT lastname FROM people WHERE lastname like 'Иванов' ORDER BY substring(lastname, 1, 10) LIMIT 1

T
SELECT DISTINCT ON(lastname) lastname FROM people WHERE lastname = 'Иванов' ORDER BY lastname DESC LIMIT 1

T
SELECT lastname FROM people WHERE lastname = 'Иванов' GROUP BY lastname ORDER BY lastname LIMIT 1

F
SELECT lastname FROM people GROUP BY lastname ORDER BY upper(lastname) DESC LIMIT 1

F
