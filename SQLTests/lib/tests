UPDATE

R

R2
SET field1 = 1, field2 = 2

R

R1
SELECT

R

R2
DISTINCT ON(field1)  FROM table1  GROUP BY field1, field2 HAVING count(field3) > 0 ORDER BY field1 DESC, field2 ASC, field3 DESC LIMIT 33

R

R1
DROP USER

R

R2
user1

