\end{tabular}

T
SELECT id, lower(lastname) FROM people ORDER BY lastname ASC, id LIMIT 1

F
SELECT id, lastname FROM people ORDER BY oid LIMIT 1

F
SELECT id, upper(lastname) FROM people WHERE lastname like 'Иванов' ORDER BY id DESC, lastname LIMIT 1

