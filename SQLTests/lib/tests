                    ON(t2.childid =
                       t1.id)  FULL OUTER JOIN people t3
                    ON(t1.id =
                       t3.childid) GROUP BY t3.firstname, t3.childid ORDER BY lower(t3.firstname) ASC LIMIT 1}

