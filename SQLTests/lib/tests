T
SELECT firstname, id, age, secondname, lastname FROM people ORDER BY oid LIMIT 1

T
SELECT firstname, id, age, secondname, lastname FROM people WHERE age in (30, 40) OR secondname like 'Иванович' ORDER BY oid LIMIT 1

T
SELECT DISTINCT firstname, id, age, secondname, lastname FROM people ORDER BY firstname ASC, age ASC LIMIT 1

F
SELECT trim(firstname), id, age, secondname, lower(lastname) FROM people ORDER BY oid LIMIT 1

F
SELECT firstname, min(id), max(age), lower(secondname), lastname FROM people GROUP BY firstname, secondname, lastname ORDER BY secondname, trim(firstname) DESC LIMIT 1

