T
SELECT id, firstname, age FROM people WHERE id = 1 GROUP BY id, firstname, age ORDER BY id ASC LIMIT 1

T
SELECT id, firstname, age FROM people WHERE firstname like 'Иван' AND age in (30, 40) ORDER BY oid LIMIT 1

T
SELECT id, trim(firstname), age FROM people WHERE firstname = 'Иван' OR id = 1 ORDER BY oid LIMIT 1

F
